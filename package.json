{
  "name": "svelte-chess-pgn",
  "version": "1.1.0",
  "jest": {
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [],
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": "./svelte.config.test.cjs"
        }
      ],
      "^.+\\.ts$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "moduleNameMapper": {
      "^\\$lib(.*)$": "<rootDir>/src/lib$1",
      "^\\$app(.*)$": [
        "./.svelte-kit/dev/runtime/app$1",
        "./.svelte-kit/build/runtime/app$1"
      ]
    },
    "setupFilesAfterEnv": [
      "./jest-setup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,svelte,js,jsx}"
    ]
  },
  "import": "./dist/index.js",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "auto"
        }
      ]
    ]
  },
  "publishConfig": {
    "directory": "dist"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "svelte": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "private": false,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-package",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.svelte .",
    "start": "sirv public --no-clear",
    "test": "jest --coverage",
    "test_svelte": "playwright test",
    "format": "prettier --write .",
    "test:watch": "npm run test -- --watchAll",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@playwright/test": "^1.33.0",
    "@sveltejs/adapter-vercel": "^2.0.4",
    "@sveltejs/kit": "1.15.2",
    "@sveltejs/package": "^2.0.1",
    "@sveltejs/site-kit": "^2.1.4",
    "@sveltejs/svelte-repl": "^0.4.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/svelte": "^3.0.0",
    "@types/marked": "^4.0.8",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.34.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "esm": "^3.2.25",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "marked": "^4.2.12",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-jester": "^1.0.5",
    "svelte-preprocess": "^5.0.1",
    "svelte2tsx": "^0.6.1",
    "tslib": "^2.5.0",
    "typescript": "~5.0.4",
    "vite": "^4.1.2"
  },
  "peerDependencies": {
    "svelte": "^3.25.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@testing-library/svelte": "^3.2.2",
    "chess.js": "^1.0.0-beta.4",
    "chessboard-element": "^1.2.0",
    "chessground": "^8.3.7",
    "jsdom": "^22.0.0",
    "lodash": "^4.17.21",
    "playwright": "^1.33.0",
    "sirv-cli": "^2.0.0",
    "ts-jest": "^29.1.0"
  },
  "keywords": [
    "svelte"
  ],
  "files": [
    "src",
    "dist"
  ],
  "type": "module"
}
